    <script>
        function GetDistrict(el) {
            let Id = $(el).val();
            $.ajax({
                url: '/Cart/GetDistrict',
                type: 'post',
                dataType: 'json',
                data: {
                    Id: Id
                },
                success: function (res) {
                    let row = `<option value="0">--Chọn Quận/ Huyện ----</option>`;
                    $(res).each(function (i, e) {
                        row += `<option value="${e.id}">${e.name}</option>`;
                    })
                    $('#billingDistrict').html('');
                    $('#billingDistrict').append(row);

                }
            })
        }
        
    </script>

